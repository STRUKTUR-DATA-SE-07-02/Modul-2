#include <iostream>
using namespace std;

int main() {
    int dim1, dim2, dim3;

    cout << "Masukkan ukuran dimensi 1: ";
    cin >> dim1;
    cout << "Masukkan ukuran dimensi 2: ";
    cin >> dim2;
    cout << "Masukkan ukuran dimensi 3: ";
    cin >> dim3;

    int arr[dim1][dim2][dim3];

    // Input nilai untuk setiap elemen array (sesuaikan dengan kebutuhan)
    for (int i = 0; i < dim1; ++i) {
        for (int j = 0; j < dim2; ++j) {
            for (int k = 0; k < dim3; ++k) {
                cout << "Masukkan nilai untuk arr[" << i << "][" << j << "][" << k << "]: ";
                cin >> arr[i][j][k];
            }
        }
    }

    // Output nilai array (sesuaikan dengan format yang diinginkan)
    for (int i = 0; i < dim1; ++i) {
        for (int j = 0; j < dim2; ++j) {
            for (int k = 0; k < dim3; ++k) {
                cout << arr[i][j][k] << " ";
            }
            cout << endl;
        }
        cout << endl;
    }

    return 0;
}